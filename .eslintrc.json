{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "semi": [2, "never"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-console": "off",
    "max-len": 0,
    "comma-dangle": 0,
    "linebreak-style": 0,
    "react/prop-types": 0,
    "no-mixed-operators": "off"
  }
}
